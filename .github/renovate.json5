{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    ":automergeDigest",
    ":automergeMinor"
  ],
  "automergeType": "branch",
  "pinDigest": {
    "automerge": true
  },
  "packageRules": [
    {
      // Always want to use latest version
      "matchDepNames": [
        "ghcr.io/amyjeanes/keypad/keypad-flasher"
      ],
      "matchUpdateTypes": [
        "pinDigest"
      ],
      "enabled": false
    }
  ]
}
